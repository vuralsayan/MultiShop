@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br /><br />
    <h3>Kullanıcı Profil Bilgisi</h3>
    <div class="alert alert-info">Kullanıcı ID: @Model.Id</div>
    <div class="alert alert-danger">Kullanıcı Adı: @Model.Username</div>
    <div class="alert alert-warning">Kullanıcı Ad Soyad: @Model.Name @Model.Surname</div>
    <div class="alert alert-success">Email Adresi: @Model.Email</div>
</div>
